  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^9.0.5"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.5",
    "@nestjs/core": "^9.0.5",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/testing": "^9.0.5",
    "@types/jest": "^26.0.14",
    "@types/supertest": "^2.0.10",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3",
    "version-bump-prompt": "^6.1.0"
  },
  "repository": {
    "type": "git",
