{
  "name": "nestjs-request-context",
  "version": "2.1.0",
  "description": "Helper library for accessing request from singleton services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/*.ts\" \"test/*.ts\"",
    "bump": "bump --commit \"release %s\"",
